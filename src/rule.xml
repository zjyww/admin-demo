<?xml version="1.0" encoding="UTF-8"?>
 <rule>
　　<sheetNo>0</sheetNo>  <!-- 非必须,默认解析所有sheet.可指定解析对应的sheet,解析多个sheet用","分隔 -->
    <start>3</start> <!-- 非必须,默认0.解析开始行坐标，从０开始 -->
    　<!-- 非必须，默认-1.解析结束行坐标,负数时候从最后一行往回索引. -->
    <cells> <!-- 每一个单元格的解析策略 -->
        <cell>
            <index>2</index>　<!-- 单元格索引,从0开始 -->
            <attribute>name</attribute>　<!-- model属性名 -->
        </cell>
        <cell>
            <index>2</index>
            <attribute>cn_name</attribute>
        </cell>
        <cell>
            <index>4</index>
            <attribute>explanation</attribute>
        </cell>
    </cells>
</rule>