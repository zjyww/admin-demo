<?xml version="1.0" encoding="UTF-8"?>
 <rule>
　　<sheetNo>3</sheetNo>  <!-- 非必须,默认解析所有sheet.可指定解析对应的sheet,解析多个sheet用","分隔 -->
    <start>3</start> <!-- 非必须,默认0.解析开始行坐标，从０开始 -->
    　<!-- 非必须，默认-1.解析结束行坐标,负数时候从最后一行往回索引. -->
    <cells> <!-- 每一个单元格的解析策略 -->
        <cell>
            <index>5</index>
            <attribute>code</attribute>
        </cell>
        <cell>
            <index>11</index>
            <attribute>electricity</attribute>
        </cell>
        <cell>
            <index>17</index>
            <attribute>price</attribute>
        </cell>
       
    </cells>
</rule>